#include "main.h"
#include <stdio.h>

/**
 * main - prints all arguments it receives.
 *
 * @argc: argument count
 * @argv: argument vector
 *
 * Return : the argument vector value.
 */

int main(int argc, char *argv[])
{
	int i;

	for (i = 0, i < argc, i++)
	{
		printf("%d\n", argv[i]);
		return (0);
	}
	return (0);
}
